function [ W ] = punctePerceptronBatch( X,D,q,f )

    [n m] = size(X);
    X(n + 1, :) = ones(1, m);
    W = rand(1, n+1);
    
    for j=1:100
        S=0;
        for i=1:m
            y = f(W * X(:,i));
            if D(1,i)~= y
                S = S + (D(1,i)-y) * X(:,i);
            end
        end
        W = W + q * S;
    end
    y = f(X'*W);
    y'
    D
end
