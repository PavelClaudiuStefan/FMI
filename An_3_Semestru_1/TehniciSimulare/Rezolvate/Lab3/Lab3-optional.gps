depozit STORAGE 2000histogram TABLE s$depozit,100,100,20unitati_necesare VARIABLE 1000 - s$depozit cerere_zilnica VARIABLE RN1@1000+40 nivel_initial EQU 1000minim_depozit EQU 800GENERATE ,,,1,3ENTER depozit nivel_initial TERMINATEGENERATE 5ASSIGN 1,v$unitati_necesareTEST L s$depozit,minim_depozit,terminatADVANCE 5ENTER depozit p1TERMINATEterminat TERMINATE GENERATE 1ASSIGN 2,v$cerere_zilnicaTABULATE histogramTEST GE s$depozit,p2,termLEAVE depozit p2TERMINATE 1term TERMINATE 1